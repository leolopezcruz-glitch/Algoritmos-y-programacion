Algoritmo CON05
	Definir Numero Como Entero;
	Escribir "Introduzca un numero: ";
	Leer Numero;
	Si Numero > 100 entonces;
		Escribir "El numero introducido es mayor que 100";
	SiNo
		Escribir "El numero introducido no es mayor que 100";
	FinSi
FinAlgoritmo
